{
	"name": "yapi-to-ts",
	"displayName": "yapi-to-ts",
	"description": "a vsCode pligin fork by yapi-to-typescript",
	"version": "0.0.1",
	"publisher": "pengian",
	"repository": {
		"url": "https://github.com/Pengian/vscode-yapi-to-ts.git"
	},
	"engines": {
		"vscode": "^1.53.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./dist/extension.js",
	"icon": "logo.png",
	"contributes": {
		"commands": [
			{
				"command": "yapi-to-ts.initConfig",
				"title": "初始化yapi配置文件"
			},
			{
				"command": "yapi-to-ts.generator",
				"title": "生成yapi接口定义文件"
			},
			{
				"command": "yapi-to-ts.last-config-to-generate",
				"title": "沿用最近yapi配置"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "yapi-to-ts.initConfig",
					"when": "false"
				},
				{
					"command": "yapi-to-ts.generator"
				},
				{
					"command": "yapi-to-ts.last-config-to-generate"
				}
			],
			"explorer/context": [
				{
					"command": "yapi-to-ts.initConfig",
					"group": "navigation@1",
					"label": "初始化yapi配置",
					"when": "false && resourceFilename == package.json"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "cross-env MODE=production webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"build": "vsce package"
	},
	"devDependencies": {
		"@types/fs-extra": "^9.0.13",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "16.x",
		"@types/to-json-schema": "^0.2.1",
		"@types/vscode": "^1.53.0",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"@vscode/test-electron": "^2.1.3",
		"cross-env": "^7.0.3",
		"eslint": "^8.16.0",
		"glob": "^8.0.3",
		"mocha": "^10.0.0",
		"ts-loader": "^9.3.0",
		"typescript": "^4.7.2",
		"webpack": "^5.72.1",
		"webpack-cli": "^4.9.2"
	},
	"dependencies": {
		"@types/json-schema": "^7.0.11",
		"change-case": "^4.1.2",
		"dayjs": "^1.11.3",
		"form-data": "^3",
		"fs-extra": "^10.1.0",
		"got": "^12.1.0",
		"json-schema-to-typescript": "^10.1.4",
		"json5": "^2.2.1",
		"prettier": "^2.2.1",
		"to-json-schema": "^0.2.5",
		"vtils": "^4.61.0",
		"yapi-to-typescript": "^3.34.0"
	}
}
